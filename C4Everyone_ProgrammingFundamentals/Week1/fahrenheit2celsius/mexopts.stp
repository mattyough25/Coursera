# stp file generated by gnumex
sub gnumexopts
{

    my @language_handled = ('C');
    my $default_location = "C:\\msys64\\mingw64\\bin";

    my $locate_fcn = sub {
        return "C:\\msys64\\mingw64\\bin";
    };
    my $root_val = sub {
        my $err_msg, $warn_msg;
        return ($err_msg, $warn_msg);
    };    

    return {
        "vendor_name"      => "gcc",
        "version"          => "",                          #This version is left blank intentionally. 
        "group_id"         => "gcc",
        "serial"           => 4,
        "root_var"         => "GCCINSTALLDIR",
        "optfile_name"     => "GNUMEXOPTS.bat",
        "default_location" => $default_location,
        "language_handled" => \@language_handled,
        "root_val"         => $root_val,
        "locate"           => $locate_fcn,
        };
}
1;
